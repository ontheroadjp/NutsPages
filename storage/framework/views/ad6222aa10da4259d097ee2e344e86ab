<script>
<?php if(Session::has('alert_success')): ?>
    nutsAlertSuccess( "<?php echo e(Session::get('alert_success')); ?>" );
<?php elseif(Session::has('alert_info')): ?>
    nutsAlertInfo( "<?php echo e(Session::get('alert_info')); ?>" );
<?php elseif(Session::has('alert_danger')): ?>
    nutsAlertDanger( "<?php echo e(Session::get('alert_danger')); ?>" );
<?php elseif(Session::has('alert_warning')): ?>
